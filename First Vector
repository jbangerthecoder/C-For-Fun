#include <vector>
#include <iostream>
using namespace std;

int main () {



    vector <int> vec{10,20,30,40,50};
    vec.at(0) = 100;
    vec.at(4) = 1000;
    
    cout <<"\nIndex 0: " << vec.at(0);
    cout <<"\nIndex 1: " << vec.at(1);
    cout <<"\nIndex 2: " << vec.at(2);
    cout <<"\nIndex 3: " << vec.at(3);
    cout <<"\nIndex 4: " << vec.at(4);

 
    return 0;
}
